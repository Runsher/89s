<script type="text/javascript">
function returndata(data) {
    var parseData = "";
    with(data.weatherinfo) {
		parseData+="<a href='http://www.weather.com.cn/weather/101010100.shtml'>"+city+"</a>";
		parseData+="<img src='http://i2.chinanews.com/weather/img/c" + (img2 == 99 ? img1: img2) + ".gif' title='中国天气网今日天气指数 紫外线+指数:" + index_uv + "。穿衣指数+:" + index_d+"' />",
		parseData+="<a href='http://www.weather.com.cn/weather/101010100.shtml'>"+temp1+"</a>";
		// parseData+="<a href='http://www.weather.com.cn/weather/101010100.shtml'>"+wind1+"</a>";

    }
    return parseData;
}

function getData(){
	var htmlobj=$.ajax({
        	type:'get',	
        	async:false,
			url:'/iframe/static/weather_json.html',
			dataType: "json",
			success:function(data){

				$("#info").html(returndata(data));
			}

		});

}

$(document).ready(function(){
	getData();

});
</script>

