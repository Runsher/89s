<script type="text/javascript">

$(function(){
	var array = new Array(["gdt","2"],["rdht","3"],["shmid","44"],["shleft","43"],["wyright","42"],["wymid","41"],["wyleft","40"],["cjright","39"],["cjmid","38"],["cjleft","37"],["jsright","36"],["jsmid","35"],["jsleft","34"],["gjright","33"],["gjmid","32"],["gjleft","31"],["hrright","30"],["hrmid","29"],["hrleft","28"],["gnright","27"],["gnmid","26"],["gnleft","25"],["picyaowen","20"],["paihangbiao","19"],["yaowen2","18"],["ecns","17"],["wangyouhudong","15"],["yaowen1","14"],["daodu","6"],["shright","45"],["daohang","46"],["tyleft","61"],["tymid","62"],["tyright","63"],["xwft","64"],["pkjm","65"],["jsxw","66"]);
	for(var i=0;i<array.length;i++){
		var itemid = array[i][0];
		var itempaid = array[i][1];
		$("#"+itemid).find("a").each(function(){
			var temppaid = itempaid;
			$(this).click(function(){
				var url = $(this).attr("href");
				if(url!='#' && url!=''){
					$.ajax({

					  type: "GET",
					  url: "http://lc.chinanews.com:8090/rpc/pa.jsp?pid=1&paid="+temppaid+"&u="+url+"&aj="+encodeURI(Math.random()+ (new Date())),
					  dataType: "jsonp"
					});
				}
			});
		});
	}

$(".bigimgdiv95812").bind("click",function(){
			$.ajax({
			  type: "GET",
			  url: "http://lc.chinanews.com:8090/rpc/pa.jsp?pid=1&paid=16&u=&aj="+encodeURI(Math.random()+ (new Date())),
			  dataType: "jsonp"
			});
	}); 

});

 var mynum=window.location.search;
if(mynum ==''){
  	var sUserAgent = navigator.userAgent;
  	var mobileAgents = ['Windows CE', 'iPod', 'Symbian', 'iPhone', 'BlackBerry', 'Android'];

  	if (sUserAgent.indexOf('Android') > -1 && (sUserAgent.indexOf('ERD79') > -1 || sUserAgent.indexOf('MZ60') > -1 || sUserAgent.indexOf('GT-P7') > -1 || sUserAgent.indexOf('SCH-P7') > -1)) {

  	} else {

  		if (location.href.indexOf('?pc') == -1) {

  			for (var i = 0; i < mobileAgents.length; i++) {

  				if (sUserAgent.indexOf(mobileAgents[i]) > -1) {

  					location = 'http://m.chinanews.com/';

  					break;

  				}

  			}

  		} 
  	}

  	var ads = [];
}
